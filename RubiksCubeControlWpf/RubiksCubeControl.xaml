<UserControl x:Class="RubiksCubeControlWpf.RubiksCubeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RubiksCubeControlWpf"
             xmlns:shapes="clr-namespace:RubiksCubeControlWpf.Shapes"
             mc:Ignorable="d"  
             Background="Transparent"         
             DataContext="{Binding Mode=OneWay, RelativeSource={RelativeSource Self}}"
             Width="546"
             Height="540" 
             Margin="0"
             Padding="0"
             x:Name="rubiksCubeUserControl"
             HorizontalAlignment="Center" VerticalAlignment="Center"
             HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
             >

    <UserControl.Resources>

        <SolidColorBrush x:Key="ControlDarkColorBrush" Color="{x:Static SystemColors.ControlDarkColor}" />
        <SolidColorBrush x:Key="ControlColorBrush" Color="{x:Static SystemColors.ControlColor}" />

        <Style x:Key="canvasGroup" TargetType="Canvas">
            <Setter Property="Width" Value="Auto" />
            <Setter Property="Height" Value="Auto" />
        </Style>

        <Style x:Key="animationPathStyle" TargetType="Shape">
            <Setter Property="Stroke" Value="Black" />
            <Setter Property="StrokeThickness" Value="1" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
        </Style>

        <Style x:Key="peice" TargetType="shapes:Circle">
            <Setter Property="Radius" Value="9" />
            <Setter Property="Fill" Value="Transparent" />
            <Setter Property="Margin" Value="-9,-9,0,0" />
            <Setter Property="Stretch" Value="Fill" />
            <Setter Property="StrokeThickness" Value="9" />
        </Style>

        <Style x:Key="EndMarkers" TargetType="shapes:Circle">
            <Setter Property="Stroke" Value="Black" />
            <Setter Property="Radius" Value="2" />
            <Setter Property="Fill" Value="Transparent" />
            <Setter Property="Opacity" Value="0" />
            <d:Setter Property="Opacity" Value="0.5" />
            <Setter Property="Margin" Value="-2,-2,0,0" />
        </Style>

        <Style x:Key="EndMarkers_" TargetType="shapes:Circle">
            <Setter Property="Stroke" Value="White" />
            <Setter Property="Fill" Value="Transparent" />
            <Setter Property="Opacity" Value="0" />
            <d:Setter Property="Opacity" Value="0.8" />
            <Setter Property="Radius" Value="1" />
            <Setter Property="Margin" Value="-1,-1,0,0" />
        </Style>

        <Style x:Key="EndMarkers__" TargetType="shapes:Circle">
            <Setter Property="Stroke" Value="White" />
            <Setter Property="Fill" Value="Transparent" />
            <Setter Property="Opacity" Value="0" />
            <d:Setter Property="Opacity" Value="1" />
            <Setter Property="Radius" Value="0.1" />
            <Setter Property="Margin" Value="-0.1,-0.1,0,0" />
        </Style>


        <Style TargetType="Path">
            <Setter Property="Fill" Value="Transparent" />
            <Setter Property="Opacity" Value="0.1" />
            <Setter Property="Stroke"   Value="Transparent" />
        </Style>

        <Style x:Key="RingPathStyle" TargetType="Path">
            <Setter Property="Fill" Value="Transparent" />
            <Setter Property="Opacity" Value="0" />
            <d:Setter Property="Opacity" Value="0" />
        </Style>

        <Style x:Key="RingPathStyle_Highlight" TargetType="Path">
            <Setter Property="Fill" Value="Transparent" />
            <Setter Property="Opacity" Value="0" />
            <d:Setter Property="Opacity" Value="0" />
        </Style>

        <Style x:Key="HintHelperStyle" TargetType="Label">
            <Setter Property="Foreground" Value="Black" />
            <Setter Property="FontFamily" Value="Engravers MT" />
            <Setter Property="FontWeight" Value="DemiBold" />
            <Setter Property="FontStyle" Value="Normal" />
            <Setter Property="FontSize" Value="32" />
        </Style>

    </UserControl.Resources>

    <Border x:Name="ControlBorder" 
            BorderBrush="DarkSlateGray" Background="{DynamicResource ControlDarkColorBrush}" 
            MinWidth="546"
            MinHeight="540"
            BorderThickness="3" CornerRadius="520,520,320,320"  
            Margin="0" Padding="0"
            Opacity="1" Focusable="False" 
            HorizontalAlignment="Center"
            VerticalAlignment="Center"             
            >

        <Grid Margin="0">

            <Canvas x:Name="CubeNet" Style="{DynamicResource canvasGroup}" Margin="50,50,50,65" MouseLeftButtonDown="CubeNet_MouseLeftButtonDown" >


                <WrapPanel Canvas.Left="402" Canvas.Top="-45">

                    <Button x:Name="helpButton" Click="helpButton_Click" 
                        Content="s" FontFamily="Marlett" FontSize="20"
                        Background="{DynamicResource ControlColorBrush}" 
                        Foreground="Black" BorderThickness="1.75" Padding="2"
                        Width="27" Height="27"                        
                        Focusable="True"  />
                    
                    <Button x:Name="closeButton" Click="closeButton_Click" 
                        Content="r" FontFamily="Marlett" FontSize="20" FontWeight="Bold"
                        Background="{DynamicResource ControlColorBrush}" 
                        Foreground="Black" BorderThickness="1.75" Padding="2"
                        Width="27" Height="27"                        
                        Focusable="True"  />


                </WrapPanel>


